{
  "kind": "collectionType",
  "collectionName": "program",
  "info": {
    "singularName": "programs",
    "pluralName": "program",
    "displayName": "Programs",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "year": {
      "type": "integer"
    },
    "availability": {
      "type": "boolean"
    },
    "processed": {
      "type": "boolean"
    },
    "duration": {
      "type": "biginteger"
    },
    "name": {
      "type": "string"
    },
    "enumeration": {
      "type": "enumeration",
      "enum": [
        "season",
        "episode",
        "movie",
        "clip",
        "series",
        "format"
      ]
    },
    "episodeNumber": {
      "type": "integer"
    },
    "seasonNumber": {
      "type": "integer"
    },
    "studioAssetID": {
      "type": "string"
    },
    "seoTitle": {
      "type": "string"
    },
    "parentStudioAssetID": {
      "type": "string"
    },
    "contentPartner": {
      "type": "string"
    },
    "studio": {
      "type": "string"
    },
    "mediaInfoOutput": {
      "type": "json"
    },
    "genreText": {
      "type": "string"
    },
    "imdbID": {
      "type": "string"
    },
    "childrenOrderedIDs": {
      "type": "json"
    },
    "parentPublishStatus": {
      "type": "json"
    },
    "enforceHls": {
      "type": "boolean"
    },
    "streamQuality": {
      "type": "enumeration",
      "enum": [
        "SD_420p",
        "HD_720p",
        "FHD_1080p",
        "UHD_2160p"
      ]
    },
    "people": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::persons.persons",
      "mappedBy": "program"
    },
    "genres": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::genres.genres",
      "mappedBy": "program"
    },
    "pg_ratings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pg-ratings.pg-ratings",
      "mappedBy": "program"
    },
    "asset": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
