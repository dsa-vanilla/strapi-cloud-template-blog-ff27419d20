{
  "kind": "collectionType",
  "collectionName": "category",
  "info": {
    "singularName": "categories",
    "pluralName": "category",
    "displayName": "Categories",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "name": {
      "type": "string"
    },
    "ssoTitle": {
      "type": "string"
    },
    "templateType": {
      "type": "enumeration",
      "enum": [
        "hero",
        "editorial",
        "promo",
        "latest",
        "genre",
        "query",
        "resumeWatching"
      ]
    },
    "packshotType": {
      "type": "enumeration",
      "enum": [
        "landscape",
        "portrait"
      ],
      "default": "landscape"
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::pages.pages",
      "mappedBy": "category"
    },
    "asset": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
